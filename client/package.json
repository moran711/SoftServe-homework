{
  "name": "site",
  "version": "1.0.0",
  "description": "softserve homework site",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ],
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "pretest": "eslint --ignore-path .gitignore . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run pretest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moran711/SoftServe-homework.git"
  },
  "keywords": [
    "html",
    "css",
    "scss",
    "js",
    "webpack"
  ],
  "author": "Roman Romaniv <romaniv788@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moran711/SoftServe-homework/issues"
  },
  "homepage": "https://github.com/moran711/SoftServe-homework#readme",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^7.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "pretty-quick": "^3.1.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.1.2"
  }
}
